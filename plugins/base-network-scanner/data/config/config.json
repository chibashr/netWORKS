{
  "max_scan_history": 50,
  "default_scan_timeout": 5,
  "default_scan_retries": 2,
  "default_parallel_hosts": 25,
  "default_scan_type": "ping",
  "show_scan_history": true,
  "scan_templates": {
    "ping_sweep": {
      "name": "Ping Sweep",
      "description": "Basic ping sweep to discover live hosts",
      "discovery_method": "ping",
      "timeout": 2,
      "retries": 2,
      "parallel": 25
    },
    "quick_scan": {
      "name": "Quick Scan",
      "description": "Fast scan of common ports and basic host information",
      "discovery_method": "ping",
      "timeout": 2,
      "retries": 1,
      "parallel": 25,
      "port_scan_type": "connect",
      "use_common_ports": true,
      "port_group": "top100"
    },
    "deep_scan": {
      "name": "Deep Scan",
      "description": "Comprehensive scan with service detection and OS fingerprinting",
      "discovery_method": "all",
      "timeout": 5,
      "retries": 2,
      "parallel": 10,
      "port_scan_type": "syn",
      "use_common_ports": true,
      "port_group": "top1000",
      "os_detection": true,
      "service_detection": true
    },
    "manual_scan": {
      "name": "Manual Scan",
      "description": "Configurable custom scan",
      "discovery_method": "ping",
      "timeout": 2,
      "retries": 2,
      "parallel": 25,
      "manual": true
    }
  },
  "custom_range": "10.1.9.1-254"
}